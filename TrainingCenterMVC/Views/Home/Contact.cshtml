@model TrainingCenterMVC.Models.Contact

@{
    ViewBag.Title = "Contact";
}



<!DOCTYPE html>
<html>

<head>
    <title>Contact</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <!-- Fonts-->
    <link href="~/Content/assets/fonts/fontawesome/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/assets/fonts/themify-icons/themify-icons.css" rel="stylesheet" />
    <link href="~/Content/assets/vendors/bootstrap4/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/assets/vendors/magnific-popup/magnific-popup.css" rel="stylesheet" />

    <link href="~/Content/assets/vendors/owl.carousel/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/main.css" rel="stylesheet" />


    
</head>

<body>
    <div class="page-wrap" style="margin-left:20px; width:100%;">

       
        <div class="md-content">

            <!-- Section -->
            <section class="md-section js-consult-form" style="background-color:#f7f7f7;">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-6 offset-0 offset-sm-0 offset-md-0 offset-lg-3 ">

                            <!-- title-01 -->
                            <div class="title-01">
                                <h2 class="title-01__title">Contact Details</h2>
                                <div>Integer placerat ullamcorper urna nec rhoncus. Sed velit justo, lacinia non sapien imperdiet, sagitt</div>
                            </div>
                            <!-- End / title-01 -->

                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-4 ">

                            <!-- iconbox -->
                            <div class="iconbox iconbox__style-04">
                                <div class="iconbox__icon"><i class="ti-email"></i></div>
                                <div>
                                    <h2 class="iconbox__title"><a href="#">Email</a></h2>
                                    <div class="iconbox__description">info@awe7.com</div>
                                </div>
                            </div>
                            <!-- End / iconbox -->

                        </div>
                        <div class="col-lg-4 ">

                            <!-- iconbox -->
                            <div class="iconbox iconbox__style-04">
                                <div class="iconbox__icon"><i class="ti-location-pin"></i></div>
                                <div>
                                    <h2 class="iconbox__title"><a href="#">location</a></h2>
                                    <div class="iconbox__description">
                                        3919 New Cut Rd, Ellicott City,
                                        <br>Maryland, US
                                    </div>
                                </div>
                            </div>
                            <!-- End / iconbox -->

                        </div>
                        <div class="col-lg-4 ">

                            <!-- iconbox -->
                            <div class="iconbox iconbox__style-04">
                                <div class="iconbox__icon"><i class="ti-mobile"></i></div>
                                <div>
                                    <h2 class="iconbox__title"><a href="#">hotline</a></h2>
                                    <div class="iconbox__description">+1-202-555-0192</div>
                                </div>
                            </div>
                            <!-- End / iconbox -->

                        </div>
                    </div>

                    <!-- form-01 -->
                    <div class="form-01 consult-form js-consult-form__content" id="form01">
                        <h2 class="form-01__title">Give Us Your Feedback</h2>
                        @*<form class="form-01__form">*@

                        @using (Html.BeginForm("Contact", "Home", FormMethod.Post, new { @class = "form-01__form" }))
                        {
                            @Html.AntiForgeryToken()

                            <div class="form__item form__item--03">
                                @Html.EditorFor(model => model.Name, new { htmlAttributes = new { placeholder = "Your name", @class = "form-control" } })


                            </div>
                            <div class="form__item form__item--03">
                                @Html.EditorFor(model => model.Email, new { htmlAttributes = new { placeholder = "Your Email", @class = "form-control" } })
                                  </div>
                            <div class="form__item form__item--03">
                                @Html.EditorFor(model => model.Subject, new { htmlAttributes = new { placeholder = "Your Subject", @class = "form-control" } })
                                  </div>
                                <div class="form__item">
                                    <textarea rows="3" name="Your message" placeholder="Your message"></textarea>
                                </div>
                               

                                <div class="form-group">
                                    <div>
                                        <input type="submit" value="Send Message" class="btn btn-primary btn-w180" />
                                    </div>
                                </div>
                            
                       
                        <div>
                            @Html.ActionLink("Back to List", "Index")
                        </div>


}
                    </div>
                    <!-- End / form-01 -->

                </div>
            </section>
            <!-- End / Section -->
            <!-- Section -->
            <section class="md-section">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-6 offset-0 offset-sm-0 offset-md-0 offset-lg-3 ">

                            <!-- title-01 -->
                            <div class="title-01 title-01__style-05">

                            </div>
                            <!-- End / title-01 -->

                        </div>
                    </div>
                </div>
            </section>

        </div>
        
    </div>
    <!-- Vendors-->
    <script src="~/Content/assets/vendors/jquery/jquery.min.js"></script>
    <script src="~/Content/assets/vendors/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="~/Content/assets/vendors/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/Content/assets/vendors/jquery.countdown/jquery.countdown.min.js"></script>
    <script src="~/Content/assets/vendors/jquery.countTo/jquery.countTo.min.js"></script>
    <script src="~/Content/assets/vendors/jquery.countUp/jquery.countup.min.js"></script>
    <script src="~/Content/assets/vendors/jquery.matchHeight/jquery.matchHeight.min.js"></script>
    <script src="~/Content/assets/vendors/jquery.mb.ytplayer/jquery.mb.YTPlayer.min.js"></script>
    <script src="~/Content/assets/vendors/magnific-popup/jquery.magnific-popup.min.js"></script>
  <script src="~/Content/assets/vendors/masonry-layout/masonry.pkgd.min.js"></script>
    <script src="~/Content/assets/vendors/owl.carousel/owl.carousel.min.js"></script>
    <script src="~/Content/assets/vendors/jquery.waypoints/jquery.waypoints.min.js"></script>
    <script src="~/Content/assets/vendors/menu/menu.min.js"></script>
    <script src="~/Content/assets/js/main.js"></script>

</body>

</html>













